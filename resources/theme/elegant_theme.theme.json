{
  "name": "Elegant Theme",
  "dark": true,
  "author": "yx208",
  "editorScheme": "/theme/elegant_theme.icls",
  "colors": {
    "dark": "#26292E",

    "gray": "#30353d",
    "gray2": "#2d3138",
    "gray3": "#383e47",
    "gray4": "#5B697C",

    "primary": "#085dc2",
    "primarySecond": "#2077f1",

    "pureWhite": "#ffffff",
    "white": "#d0d1da",
    "white2": "#A9B7C6",

    "red": "#e80854",
    "redDeep": "#990536",

    "green": "#0AA871",
    "greenDeep": "#008F5D"
  },
  "ui": {
    "*": {
      "background": "dark",
      "inactiveBackground": "gray",
      "foreground": "white",

      "borderColor": "gray",
      "disabledBorderColor": "gray",
      "separatorColor": "gray",

      "infoForeground": "gray4",
      "selectionBackground": "gray",
      "selectionInactiveBackground": "gray",
      "selectionBackgroundInactive": "gray"
    },
    "Borders": {
      "color": "gray",
      "ContrastBorderColor": "gray3"
    },
    "List": {
      "selectionBackground": "primary",
      "selectionForeground": "white"
    },
    "RunWidget": {
      "foreground": "white",
      "separatorColor": "gray",
      "hoverBackground": "primary"
    },
    "CheckBoxMenuItem.selectionBackground": "primary",
    "Button": {

      "startBackground": "gray3",
      "endBackground": "gray3",
      "startBorderColor": "gray3",
      "endBorderColor": "gray3",

      "default": {
        "foreground": "white",
        "startBackground": "primary",
        "endBackground": "primary",
        "startBorderColor": "primary",
        "endBorderColor": "primary",
        "focusedBorderColor": "white"
      }
    },
    "Tree": {
      "selectionBackground": "primary",
      "selectionInactiveBackground": "gray",
      "selectionForeground": "pureWhite",
      "rowHeight": 26
    },
    "EditorPane.inactiveBackground": "dark",
    "CompletionPopup": {
      "foreground": "white2",
      "matchForeground": "primarySecond",
      "selectionBackground": "gray2",
      "selectionInactiveBackground": "gray2"
    },
    "EditorTabs": {
      "underlineColor": "primarySecond",
      "inactiveUnderlineColor": "gray4",
      "underlineHeight": 2,
      "underlinedTabBackground": "gray"
    },
    "ToolWindow": {
      "Header": {
        "background": "dark",
        "inactiveBackground": "dark"
      }
    },
    "WelcomeScreen.separatorColor": "gray",
    "DefaultTabs": {
      "hoverBackground": "gray",
      "underlineColor": "primarySecond",
      "underlineHeight": 2
    },
    "TabbedPane": {
      "underlineColor": "primarySecond",
      "focusColor": "gray",
      "hoverColor": "gray"
    },
    "Link.activeForeground": "primarySecond",
    "Table": {
      "stripeColor": "gray",
      "lightSelectionForeground": "white",
      "lightSelectionInactiveForeground": "white",
      "lightSelectionBackground": "gray",
      "lightSelectionInactiveBackground": "gray"
    },
    "Plugins": {
      "SectionHeader.background": "gray",
      "tagBackground": "primarySecond",
      "tagForeground": "white",

      "Button": {
        "installForeground": "white2",
        "installBorderColor": "white2",
        "installFillForeground": "white",
        "installFillBackground": "primarySecond",
        "updateForeground": "white",
        "updateBackground": "primarySecond"
      },

      "lightSelectionBackground": "gray",
      "hoverBackground": "gray"
    },
    "ProgressBar": {
      "trackColor": "white",
      "progressColor": "primarySecond",
      "indeterminateStartColor": "primarySecond",
      "indeterminateEndColor": "primary",

      "failedColor": "red",
      "failedEndColor": "redDeep",
      "passedColor": "green",
      "passedEndColor": "greenDeep"
    },
    "ActionButton": {
      "pressedBackground": "gray3",
      "pressedBorderColor": "gray3",
      "hoverBackground": "gray3",
      "hoverBorderColor": "gray3"
    },
    "Menu.selectionBackground": "primary",
    "NotificationsToolwindow": {
      "newNotification.hoverBackground": "gray",
      "newNotification.background": "gray",
      "Notification.hoverBackground": "gray"
    },
    "FileColor": {
      "Yellow": "dark",
      "Green": "dark",
      "Blue": "dark",
      "Violet": "dark",
      "Orange": "dark",
      "Rose": "dark"
    }
  },
  "icons": {
    "ColorPalette": {
      "Checkbox.Background.Default.Dark": "gray3",
      "Checkbox.Border.Default.Dark": "gray3",
      "Checkbox.Foreground.Selected.Dark": "white",
      "Checkbox.Focus.Wide.Dark": "primary",
      "Checkbox.Focus.Thin.Default.Dark": "primary",
      "Checkbox.Focus.Thin.Selected.Dark": "white",
      "Checkbox.Background.Disabled.Dark": "gray3",
      "Checkbox.Border.Disabled.Dark": "gray3",
      "Checkbox.Foreground.Disabled.Dark": "#a56a01"
    }
  }
}